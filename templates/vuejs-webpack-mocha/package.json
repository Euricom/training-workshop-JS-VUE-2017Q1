{
    "name": "webpack-template",
    "version": "1.0.0",
    "description": "WebPack Template",
    "scripts": {
        "serve-dashboard": "webpack-dashboard -- webpack-dev-server --open --inline --hot",
        "serve": "webpack-dev-server --open --inline --hot",
        "build": "cross-env NODE_ENV=production webpack",
        "test": "npm run test-ci -- --watch",
        "test-ci": "mocha-webpack --require test/setup.js --webpack-config webpack/test.config.js --recursive --glob *.spec.js app",
        "test-ci2": "mocha-webpack --require test/setup.js --webpack-config webpack/test.config.js app/**/*.spec.js"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "vue": "^2.1.10"
    },
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-core": "^6.21.0",
        "babel-loader": "^6.2.10",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-preset-es2017-node7": "^0.4.1",
        "babel-preset-latest": "^6.16.0",
        "babel-preset-stage-2": "^6.18.0",
        "chai": "^3.5.0",
        "cross-env": "^3.1.4",
        "css-loader": "^0.26.1",
        "eslint": "^3.13.1",
        "eslint-config-standard": "^6.2.1",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-standard": "^2.0.1",
        "extract-text-webpack-plugin": "^2.0.0-beta",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.4",
        "jsdom": "^9.9.1",
        "json-loader": "^0.5.4",
        "less": "^2.7.2",
        "less-loader": "^2.2.3",
        "mocha": "^3.2.0",
        "mocha-loader": "^1.1.0",
        "mocha-webpack": "^1.0.0-beta",
        "null-loader": "^0.1.1",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "vue-loader": "^10.0.2",
        "vue-template-compiler": "^2.1.10",
        "webpack": "2.2.0",
        "webpack-dashboard": "^0.2.1",
        "webpack-dev-server": "2.2.0",
        "webpack-merge": "^2.4.0",
        "webpack-node-externals": "^1.5.4"
    }
}
